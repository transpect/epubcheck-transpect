<?xml version="1.0" encoding="UTF-8"?>
<c:param-set xmlns:c="http://www.w3.org/ns/xproc-step">
  <c:param name="result-max-mb_warning" value="12"/>
  <c:param name="result-max-mb_error" value="120"/>
  <c:param name="html-max-kb_error" value="300"/>
  <!-- because some people might check against a limit of 300 * 1000 bytes instead of 300 * 1024 bytes: -->
  <c:param name="html-max-kb_warning" value="292"/>
  <c:param name="image-max-mb" value="1.5"/>
  <!-- 1 Mpx = 1e6 px -->
  <c:param name="image-max-mpx_warning" value="3.2"/>
  <c:param name="image-max-mpx_error" value="4"/>
  <!-- image-dpi: Will require all images that have density information to report this as XXXdpi,
    otherwise unc:paramed. Desirable: calculation depending on CSS settings, typical screen sizes, pixel count. -->
  <c:param name="image-min-dpi_warning" value="144"/>
  <c:param name="image-min-dpi_error" value="72"/>
  <c:param name="image-max-dpi_warning" value="300"/>
  <c:param name="cover-min-width-px" value="800"/>
  <c:param name="cover-max-width-px" value="1440"/>
</c:param-set>
